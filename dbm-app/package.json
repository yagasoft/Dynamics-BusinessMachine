    "@azure/msal-angular": "^3.0.25",
    "@azure/msal-browser": "^3.26.1",
    "@codemirror/lang-javascript": "^6.2.1",
    "@types/codemirror": "^5.60.15",
    "codemirror": "^6.0.1",
