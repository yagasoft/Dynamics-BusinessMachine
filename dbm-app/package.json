{
  "name": "dbm-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "bundle": "npx webpack",
    "start-wp": "webpack-dev-server --config webpack.config.js --host localhost --port 8080 --open",
    "build": "cd %INIT_CWD% && ng build --output-hashing=none --optimization --aot",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build-lib": "cd %INIT_CWD%/../crm-parser-editor/projects/ys-crm-parser-editor-control && ng build --watch",
    "build-bundle-prod": "cd %INIT_CWD% && ng build && npx webpack",
    "build-bundle": "cd %INIT_CWD% && ng build --configuration development && npx webpack",
    "build-bundle-watch": "cd %INIT_CWD% && ng build --configuration development && npx webpack --watch",
    "test-editor": "cd %INIT_CWD% && ng serve --open"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.2",
    "@angular/common": "^18.0.2",
